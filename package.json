  "license": "MIT",
  "repository": "https://github.com/hetznercloud/ngx-translate-mock",
  "scripts": {
    "ng": "ng",
    "build": "ng build && yarn copy:readme",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "pretty": "prettier --list-different --config ./.prettierrc \"**/*.@(js|ts|scss|md|json|yml|html)\"",
    "pretty:write": "prettier --write --config ./.prettierrc \"**/*.@(js|ts|scss|md|json|yml|html|template)\"",
    "copy:readme": "copyfiles -f ./README.md ./dist/ngx-translate-mock"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.2",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular/cli": "13.2.2",
    "@angular/compiler": "13.2.2",
    "@angular/compiler-cli": "13.2.2",
    "@angular/core": "13.2.2",
    "@ngx-translate/core": "14.0.0",
    "@types/node": "12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "copyfiles": "2.4.1",
    "eslint": "8.2.0",
    "ng-packagr": "13.0.0",
    "prettier": "2.5.1",
    "rxjs": "6.6.3",
    "tslib": "2.3.0",
    "typescript": "4.4.4",
    "zone.js": "0.11.4"
  }
}
