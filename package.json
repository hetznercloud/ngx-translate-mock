    "@angular/cli": "~7.3.2",
    "@angular/compiler": "^7.2.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.16.4",
    "ts-node": "~7.0.0",
